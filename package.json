{
  "name": "@yearn/pricing",
  "version": "1.0.0",
  "description": "Yearn Finance pricing service - fetches and serves token prices from multiple sources",
  "scripts": {
    "build": "tsc && tsc-alias",
    "refresh": "tsx src/refresh.ts",
    "refresh-route": "tsx src/refresh-route.ts",
    "dev:server": "bun run api/devserver.ts",
    "vercel": "vercel dev",
    "deploy": "vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "analyze": "tsx scripts/analyze-sources.ts"
  },
  "keywords": [
    "yearn",
    "defi",
    "pricing",
    "blockchain"
  ],
  "author": "Yearn Finance",
  "license": "MIT",
  "dependencies": {
    "@upstash/redis": "^1.35.3",
    "axios": "^1.6.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "p-limit": "^4.0.0",
    "viem": "^2.37.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/node": "^5.3.17",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vercel": "^46.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
